{
  "version": "1.0.0",
  "scripts": {
    "start": "vite",
    "start:ipfs": "VITE_HASH_ROUTING=1 vite --base ./",
    "serve": "vite preview",
    "build": "vite build && scripts/copy-katex-assets && scripts/install-twemoji-assets",
    "build:ipfs": "VITE_HASH_ROUTING=1 vite build --base ./",
    "postinstall": "scripts/copy-katex-assets && scripts/install-twemoji-assets",
    "check": "scripts/check",
    "format": "npx prettier '**/*.@(ts|js|svelte|json|css|html|yml)' --write",
    "test:unit": "TZ='UTC' vitest run",
    "test:e2e": "TZ='UTC' playwright test",
    "test:e2e:ipfs": "TZ='UTC' playwright test ./tests/e2e/hashRouter.spec.ts --config playwright.ipfs.config.ts",
    "test:httpd-api:unit": "TZ='UTC' vitest run --config httpd-client/vite.config.ts --reporter verbose"
  },
  "type": "module",
  "engines": {
    "node": ">=18.17.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.38.1",
    "@sinonjs/fake-timers": "11.0.0",
    "@sveltejs/vite-plugin-svelte": "^2.4.6",
    "@tsconfig/svelte": "^5.0.2",
    "@types/dompurify": "^3.0.3",
    "@types/katex": "^0.16.3",
    "@types/lodash": "^4.14.199",
    "@types/md5": "^2.3.3",
    "@types/node": "^18.17.1",
    "@types/sinon": "^10.0.18",
    "@types/sinonjs__fake-timers": "^8.1.2",
    "@types/wait-on": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "chalk": "^5.3.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-svelte": "^2.34.0",
    "execa": "^8.0.1",
    "exit-hook": "^4.0.0",
    "get-port": "^7.0.0",
    "happy-dom": "^12.9.0",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "svelte-check": "^3.5.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vitest": "^0.34.6",
    "wait-on": "^7.0.1"
  },
  "dependencies": {
    "@radicle/gray-matter": "4.1.0",
    "@wooorm/starry-night": "^3.0.0",
    "async-mutex": "^0.4.0",
    "baconjs": "^3.0.17",
    "bs58": "^5.0.0",
    "buffer": "^6.0.3",
    "dompurify": "^3.0.6",
    "hast-util-to-dom": "^4.0.0",
    "hast-util-to-html": "^9.0.0",
    "lodash": "^4.17.21",
    "marked": "^9.1.0",
    "marked-katex-extension": "^4.0.1",
    "marked-linkify-it": "^3.1.4",
    "md5": "^2.3.0",
    "plausible-tracker": "^0.3.8",
    "sinon": "^16.1.0",
    "svelte": "^4.2.1",
    "twemoji": "^14.0.2",
    "zod": "^3.22.4"
  }
}
